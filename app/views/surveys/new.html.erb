<%= form_for [@user,@survey] do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.hidden_field :creator_id, value: @user.id %>
  </p>
  <%= f.fields_for :questions do |builder| %>
    <%= render "question_fields", f: builder %>
  <% end %>
  <p> <%= f.submit "Submit" %> </p>
<% end %>
